{
  "name": "generator-videojs-plugin",
  "version": "0.1.2",
  "description": "A Yeoman generator for VideoJS plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "bin": {
    "videojs-plugin": "bin/videojs-plugin.sh",
    "videojs-plugin-validate": "bin/videojs-plugin-validate.js",
    "vjsp": "bin/videojs-plugin.sh",
    "vjspv": "bin/videojs-plugin-validate.js"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "VideoJS",
    "url": "https://github.com/videojs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:generator": "babel --ignore templates/**/* src/app -d generators/app && npm run build:generator:copy",
    "build:generator:copy": "cp -r src/app/templates generators/app/templates",
    "build:test": "babel src/test -d es5-test",
    "build:validate": "babel src/validate -d es5-validate",
    "clean": "rm -rf generators es5-test es5-validate && mkdir -p generators es5-test es5-validate",
    "docs": "doctoc README.md docs/*.md",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha es5-test/*.test.js"
  },
  "keywords": [
    "yeoman-generator",
    "videojs",
    "videojs-plugin"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "glob": "^6.0.1",
    "lodash": "^3.10.1",
    "npm-run-all": "~1.2.0",
    "tap-parser": "^1.2.2",
    "tape": "^4.2.2",
    "yeoman-generator": "^0.20.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "doctoc": "^0.15.0",
    "mocha": "^2.3.4",
    "tsml": "^1.0.1"
  }
}
