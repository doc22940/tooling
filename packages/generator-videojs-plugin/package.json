{
  "name": "generator-videojs-plugin",
  "version": "7.1.3",
  "description": "A Yeoman generator for Video.js plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "engine": {
    "node": ">=8"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "Pat O'Neill",
    "email": "pgoneill@gmail.com",
    "url": "https://github.com/misteroneill"
  },
  "vjsstandard": {
    "ignore": [
      "docs",
      "generators/app/templates"
    ]
  },
  "scripts": {
    "docs": "doctoc README.md docs/conventions.md",
    "lint": "vjsstandard",
    "pretest": "npm run lint",
    "test": "npm-run-all test:*",
    "test:unit": "mocha test/*.test.js",
    "test:integration": "node scripts/run-integration-tests.js",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "precommit": "lint-staged",
    "prepublish": "in-install && npm-merge-driver install || not-in-install"
  },
  "lint-staged": {
    "*.js": [
      "vjsstandard --fix",
      "git add"
    ],
    "(README.md|docs/conventions.md)": [
      "doctoc",
      "git add"
    ]
  },
  "keywords": [
    "yeoman-generator",
    "videojs",
    "videojs-plugin"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "lodash": "^4.17.10",
    "tsmlj": "^1.0.0",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.1",
    "conventional-changelog-videojs": "^3.0.0",
    "doctoc": "^1.3.1",
    "fs-extra": "^7.0.0",
    "husky": "^0.14.3",
    "in-publish": "^2.0.0",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "not-prerelease": "^1.0.1",
    "npm-merge-driver": "^2.3.5",
    "npm-run-all": "^4.1.3",
    "videojs-standard": "^7.0.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "optionalDependencies": {
    "global": "^4.3.2",
    "jsdoc": "https://github.com/BrandonOCasey/jsdoc#feat/plugin-from-cli",
    "karma": "^3.0.0",
    "postcss-cli": "^6.0.0",
    "rollup": "^0.64.1",
    "shx": "^0.3.2",
    "sinon": "^6.1.5",
    "video.js": "^6 || ^7",
    "videojs-generate-karma-config": "~2.0.0",
    "videojs-generate-postcss-config": "~2.0.1",
    "videojs-generate-rollup-config": "~2.0.1",
    "videojs-languages": "^1.0.0"
  }
}
