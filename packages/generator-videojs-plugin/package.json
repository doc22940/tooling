{
  "name": "generator-videojs-plugin",
  "version": "2.3.0",
  "description": "A Yeoman generator for VideoJS plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "engine": {
    "node": ">=4.4.0"
  },
  "bin": {
    "vjsgenclean": "bin/videojs-generator-cleanup.js"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "Pat O'Neill",
    "email": "pgoneill@gmail.com",
    "url": "https://github.com/misteroneill"
  },
  "vjsstandard": {
    "ignore": [
      "docs",
      "generators/app/templates"
    ]
  },
  "scripts": {
    "docs": "doctoc README.md docs/conventions.md",
    "lint": "vjsstandard",
    "precommit": "npm run docs && git add README.md docs/conventions.md",
    "prepush": "npm run lint -- --errors",
    "pretest": "npm run lint",
    "test": "mocha test/*.test.js",
    "version": "conventional-changelog -p videojs -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "keywords": [
    "yeoman-generator",
    "videojs",
    "videojs-plugin"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "fs-extra": "^0.26.4",
    "lodash": "^3.10.1",
    "rimraf": "^2.6.1",
    "semver": "^5.1.0",
    "tsmlj": "^1.0.0",
    "yeoman-generator": "^0.20.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.1",
    "conventional-changelog-videojs": "^3.0.0",
    "doctoc": "^0.15.0",
    "husky": "^0.13.3",
    "mocha": "^2.3.4",
    "videojs-standard": "^6.0.0"
  }
}
