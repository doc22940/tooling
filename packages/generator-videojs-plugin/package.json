{
  "name": "generator-videojs-plugin",
  "version": "0.1.2",
  "description": "A Yeoman generator for VideoJS plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "bin": {
    "vjsp": "bin/vjsp.sh"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "VideoJS",
    "url": "https://github.com/videojs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "babel --ignore templates/**/* src/app -d generators/app && npm run build:copy",
    "build:copy": "cp -r src/app/templates generators/app/templates",
    "clean": "rm -rf generators && mkdir -p generators",
    "docs": "doctoc README.md docs/*.md",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "babel src/tests -d test && mocha"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash": "^3.10.1",
    "yeoman-generator": "^0.20.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "doctoc": "^0.15.0",
    "mocha": "^2.3.3",
    "tsml": "^1.0.1"
  }
}
